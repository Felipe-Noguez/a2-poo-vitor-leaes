package application;

public class Program {

	public static void main(String[] args) {
		
		Automovel auto = new Automovel();
		Aviao aviao = new Aviao();
		
		auto.setNomeFabricante("Dodge");
		auto.setNome("RAM 2500");
		auto.setAnoFabricacao(2022);
		auto.setPaisOrigem("EUA");
		auto.setDiametroRoda(18);
		auto.setCapacidade(5);
		
		auto.mostrarDetalhes();
		auto.acelerar();
		auto.abrirPortaMalas();
		
		aviao.setNomeFabricante("Boeing");
		aviao.setNome("737");
		aviao.setAnoFabricacao(1990);
		aviao.setPaisOrigem("EUA");
		aviao.setNumTurbinas(24);
		aviao.setCapacidade(200);
		
		

	}

}
